{
  "name": "webrtc-video-chat",
  "version": "0.0.1",
  "description": "Learning WebRTC video and audio calls.",
  "main": "src/ui/index.js",
  "scripts": {
    "start": "export NODE_ENV=development && $(npm bin)/webpack-dev-server --mode=development",
    "build": "export NODE_ENV=production && $(npm bin)/webpack --mode=production",
    "test": "npm run flow && npm run eslint",
    "eslint": "$(npm bin)/eslint ./src/*/*.js ./src/*/*.jsx",
    "flow": "$(npm bin)/flow --quiet",
    "typed.install": "$(npm bin)/flow-typed install --libdefDir externs/",
    "typed.stub": "flow-typed create-stub --libdefDir externs/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WIVSW/webrtc-video-chat.git"
  },
  "author": "WIVSW",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WIVSW/webrtc-video-chat/issues"
  },
  "homepage": "https://github.com/WIVSW/webrtc-video-chat#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^1.0.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "flow-bin": "^0.90.0",
    "flow-typed": "^2.5.1",
    "html-webpack-plugin": "^3.2.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "webpack": "^4.28.4",
    "webpack-async-chunk-names-plugin": "^0.1.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4.3.1"
  }
}
